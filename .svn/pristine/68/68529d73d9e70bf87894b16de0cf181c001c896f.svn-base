@media screen and (min-width: 639px) and (max-width: 767px) {
	.m-sjfbdb{max-height:670px;}

}

@media screen and (max-width: 638px) {
	body{font-size: 14px;}
	.m-sjfbtab1 li{margin: 0 15px;}
	.m-sjfbtab2 li{width: 50%;}
    .m-sjfbdb{max-height:680px;}
}