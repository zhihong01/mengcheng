.is-main{overflow:hidden;}
.m-content1,.m-content2{max-width:1200px;margin:0 auto;overflow:hidden;position:relative;}
.m-content1 h1{font-size:32px;font-weight:bold;text-align:center;color:#d00d0d;margin:21px 0;}
.m-content1 ul li{float:left;width:33.3%;text-indent:15px;background:url(../images/home/squer.png) no-repeat 5px 10px;overflow: hidden;text-overflow:ellipsis;white-space: nowrap;font-size:16px;}

.m-content2{margin-top:25px;}
.m-flash{position: relative; margin-bottom: 24px; overflow: hidden;width: 670px;z-index:-1;height:400px;}
.m-flash .bd{position: relative; height: 100%;}
.m-flash .bd li{position: relative; zoom: 1;}
.m-flash .bd li img{display: block;}
.m-flash .bd li p{position: absolute; left: 0; right: 0; bottom: 0; padding: 0 110px 0 15px; height: 44px; line-height: 44px; background: rgba(0,0,0,.5); color: #fff; overflow:hidden; word-wrap: normal; white-space: nowrap; text-overflow: ellipsis;}
.m-flash .hd{position: absolute; right: 15px; bottom: 18px;}
.m-flash .hd li{float: left; margin-left: 9px; width: 8px; height: 8px; border-radius: 8px; background: #fff;}
.m-flash .hd li.on{width: 16px; background: #e41a1a;}

.m-news{width:510px;overflow:hidden;height:403px;}
.m-news .hd{background:#f5f5f5;overflow:hidden;}
.m-news .hd li{float:left;padding:10px 15px;font-size:20px;}
.m-news .hd li:hover a{color:#d00d0d;}
.m-news .hd li.on a{color:#d00d0d;font-weight:bold;}
.m-news .bd{margin-top:10px;}
.m-news .bd li{line-height:37px;font-size:16px;padding-left:20px;background:url(../images/home/icon.png) no-repeat left center;}
.m-news .bd li .date{float:right;font-size:14px;color:#999;}
.m-news .u-zhuanzai h3{font-size:16px;color:#d00d0d;}


.m-notice{overflow:hidden;clear:both;width:670px;background:#efefef;}
.m-notice div.tzgg{width:100px;height:100px;background:url(../images/home/mcx_22.jpg) no-repeat center center;float:left;}
.m-notice .tzgg a{display:block;height:100%;}
.tempWrap{margin-top:13px;}
.m-notice ul{float:right;width:570px;padding:13px 0;background:#efefef;}
.m-notice ul li{margin:0 20px;font-size:16px;background:url(../images/home/squer.png) no-repeat left center;padding-left:10px;line-height:37px;height:37px}
.m-notice ul .date{float:right;font-size:14px;color:#999;}

.m-notice2 ul li{float:left;margin-left:20px;}
.m-notice2 ul li a{display:block;}
