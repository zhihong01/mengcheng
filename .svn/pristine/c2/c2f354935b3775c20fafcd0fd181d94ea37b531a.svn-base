
.m-content1,.m-content2,.m-content3{max-width:1200px;margin:0 auto;overflow:hidden;position:relative;}
.m-hotservice h1{border-bottom:1px solid #ddd;margin-bottom:10px;font-size:20px;}
.m-hotservice h1 a{display:inline-block;padding:0 15px;border-bottom:2px solid #d00d0d;height:50px;line-height:50px;color:#d00d0d;background:url(../images/public/up.png) no-repeat center bottom;font-weight:bold;}
.m-hotfw{position:releative;padding:0 25px;}
.m-hotfw .item{width:100px;height:116px;}
.m-hotfw a{display:block;height:100%;text-align:center;}
.m-hotfw a i{display:block;background:url(../images/bsfw/bs-icon.png) no-repeat;width:62px;height:62px;margin:23px auto 10px;}
.m-hotservice .m-hotfw .owl-nav.disabled{display:block;}
.m-hotfw .owl-nav span{width:16px;height:36px;background:#888;opacity:0.6;font-size:30px;color:#fff;line-height:36px;display:inline-block;}
.m-hotfw .owl-nav .owl-prev{position:absolute;top:50%;margin-top:-30px;left:0;}
.m-hotfw .owl-nav .owl-next{position:absolute;top:50%;margin-top:-30px;right:0;}
.m-hotfw a.hf1 i{background-position:-192px -194px;}
.m-hotfw a.hf2 i{background-position:-323px -194px;}
.m-hotfw a.hf3 i{background-position:-451px -194px;}
