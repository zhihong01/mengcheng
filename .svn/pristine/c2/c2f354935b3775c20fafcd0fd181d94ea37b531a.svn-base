.m-per{width:800px;}
.m-per .hd ul{height:50px;line-height:50px;text-align:center;border-bottom:1px solid #ddd;margin-bottom:2px;}
.m-per .hd ul li{float:left;padding:0 15px;font-size:18px;}
.m-per .hd ul li.on a{color:#d00d0d;display:block;height:100%;font-weight:bold;}
.m-per .hd ul li.on{background:url(../images/public/up.png) no-repeat center bottom;border-bottom:2px solid #d00d0d;}
.m-per .bd{padding:0 12px;overflow:hidden;}
